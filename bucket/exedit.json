{
    "homepage": "http://spring-fragrance.mints.ne.jp/aviutl/",
    "description": "AviUtl Ex Edit Plugin",
    "version": "0.93",
    "license": "Freeware",
    "url": "http://spring-fragrance.mints.ne.jp/aviutl/exedit93rc1.zip",
    "hash": "642b2ad95db205ddcc7baaffd79461a109110d5a79afa62f078965dcf23f7b84",
    "depends": [
        "sudo",
        "aviutl"
    ],
    "installer": {
        "script": [
            "sudo New-Item -Value \"$dir\\exedit.anm\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.anm\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.auf\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.auf\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.aui\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.aui\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.auo\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.auo\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.cam\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.cam\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.ini\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.ini\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.scn\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.scn\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.tra\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.tra\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.txt\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.txt\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\lua.txt\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"lua.txt\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\lua51.dll\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"lua51.dll\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\lua51jit.dll\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"lua51jit.dll\" -ItemType SymbolicLink",
            "sudo New-Item -Value \"$dir\\exedit.obj\" -Path \"$scoopdir\\apps\\aviutl\\current\\plugins\" -Name \"exedit.obj\" -ItemType SymbolicLink"
        ]
    },
    "uninstaller": {
        "script": [
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.anm\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.auf\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.aui\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.auo\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.cam\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.ini\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.scn\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.tra\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.txt\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\lua.txt\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\lua51.dll\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\lua51jit.dll\").Delete()",
            "(Get-Item \"$scoopdir\\apps\\aviutl\\current\\plugins\\exedit.obj\").Delete()"
        ]
    }
}
